{
  "realm": "vault-dev",
  "enabled": true,
  "sslRequired": "none",
  "roles": {
    "realm": [
      { "name": "vault:admin", "description": "Full administrative access" },
      { "name": "vault:write", "description": "Write access to vault data" },
      { "name": "vault:read", "description": "Read access to vault data" }
    ]
  },
  "clients": [
    {
      "clientId": "vault-app",
      "enabled": true,
      "publicClient": true,
      "directAccessGrantsEnabled": true,
      "redirectUris": ["http://localhost:*"],
      "webOrigins": ["http://localhost:8080"]
    }
  ],
  "users": [
    {
      "username": "dev-user",
      "enabled": true,
      "email": "dev@localhost",
      "emailVerified": true,
      "credentials": [
        {
          "type": "password",
          "value": "dev-password",
          "temporary": false
        }
      ],
      "realmRoles": ["vault:admin", "vault:write", "vault:read"]
    }
  ]
}
